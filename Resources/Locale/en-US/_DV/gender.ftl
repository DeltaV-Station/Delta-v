gender-display = ({$gender ->
    [male] He / Him
    [female] She / Her
    [neuter] It / It
    [demimasc] He / They
    [demifemme] She / They
    [androgyne] He / She
    [pangender] Any Pronouns
    [agender] No Pronouns
    [neogenderxe] Xe / Xem
    [neogenderze] Ze / Hir
    [neogenderfae] Fae / Faer
    [neogenderey] Ey / Em
    *[other] They / Them
})
